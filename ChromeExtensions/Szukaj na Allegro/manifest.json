{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "manifest_version": 2,
    "name": "Szukaj na Allegro",
    "description": "Wyszukuje na Allegro zaznaczony tekst",
    "version": "1.0",
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*",
        "contextMenus"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "icons": {
        "16": "icon16.png",
        "24": "icon24.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}